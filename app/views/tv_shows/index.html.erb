<h1>Listing tv_shows</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tv_shows.each do |tv_show| %>
  <tr>
    <td><%= tv_show.name %></td>
    <td><%= tv_show.homepage_url %></td>
    <td><%= link_to 'Show', tv_show %></td>
  </tr>
<% end %>
</table>

